{
  "name": "movie-similarity-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "run-parallel": "^1.2.0"
  },
  "scripts": {
