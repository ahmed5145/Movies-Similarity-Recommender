{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "movie-similarity-frontend/package.json",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "movie-similarity-frontend/build/$1"
      }
    ]
  }
  